+static int
+fuse_mount_opt_proc(void *data, const char *arg, int key,
+                    struct fuse_args *outargs)
+{
+    struct mount_opts *mo = data;
+
+    switch (key) {
+
+    case KEY_ALLOW_ROOT:
+        if (fuse_opt_add_opt(&mo->kernel_opts, "allow_other") == -1 ||
+            fuse_opt_add_arg(outargs, "-oallow_root") == -1)
+            return -1;
+        return 0;
+
+    case KEY_RO:
+        arg = "ro";
+        /* fall through */
+
+    case KEY_KERN:
+        return fuse_opt_add_opt(&mo->kernel_opts, arg);
+
+    case KEY_DIO:
+          if (fuse_opt_add_opt(&mo->kernel_opts, "direct_io") == -1 ||
+              (fuse_opt_add_arg(outargs, "-odirect_io") == -1))
+            return -1;
+        return 0;
+
+    case KEY_IGNORED:
+        return 0;
+
+    case KEY_QUIET:
+        quiet_mode = 1;
+        return 0;
+
+    case KEY_VOLICON:
+    {
+        char volicon_arg[MAXPATHLEN + 32];
+        char *volicon_path = strchr(arg, '=');
+        if (!volicon_path) {
+            return -1;
+        }
+        if (snprintf(volicon_arg, sizeof(volicon_arg), 
+                     "-omodules=volicon,iconpath%s", volicon_path) <= 0) {
+            return -1;
+        }
+        if (fuse_opt_add_arg(outargs, volicon_arg) == -1) {
+            return -1;
+        }
+
+        return 0;
+    }
+
+    case KEY_HELP:
+        mount_help();
+        mo->ishelp = 1;
+        break;
+
+    case KEY_VERSION:
+        mount_version();
+        mo->ishelp = 1;
+        break;
+    }
+    return 1;
+}
