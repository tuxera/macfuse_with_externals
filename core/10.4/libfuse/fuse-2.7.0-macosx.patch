+static long
+os_version_major(void) 
+{
+    int ret = 0;
+    long major = 0;
+    char *c = NULL;
+    struct utsname u;
+    size_t oldlen;
+    int mib[2];
+
+    oldlen = sizeof(u.release);
+
+    mib[0] = CTL_KERN; 
+    mib[1] = KERN_OSRELEASE;
+    ret = sysctl(mib, 2, u.release, &oldlen, NULL, 0);
+    if (ret != 0) {
+        return -1;
+    }
+
+    c = strchr(u.release, '.');
+    if (c == NULL) {
+        return -1; 
+    }
+        
+    *c = '\0';
+            
+    errno = 0;
+    major = strtol(u.release, NULL, 10);
+    if ((errno == EINVAL) || (errno == ERANGE)) {
+        return -1;
+    }            
+                 
+    return major;
+}               
