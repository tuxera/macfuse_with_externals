+    hash_traverse(mount_hash, (int(*)())set_fuse_helper, &mi);
+    pthread_mutex_unlock(&mount_lock);
+}
+
+void
+fuse_unset_fuse_internal_np(struct fuse *f)
+{
+    pthread_mutex_lock(&mount_lock);
+    hash_traverse(mount_hash, (int(*)())unset_fuse_helper, f);
+    pthread_mutex_unlock(&mount_lock);
+}
