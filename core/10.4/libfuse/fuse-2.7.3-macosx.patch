+				fuse_put_internal_np(f);
+				goto out;
+			}
+			parent_ino = node->nodeid;
+		}
+		q = p;
+	}
+	if (node) {
+		node->size = newsize;
+		node->cache_valid = 0;
+		ret = 0;
+	}
+	fuse_put_internal_np(f);
+
+out:
+	return ret;
+}
